<div class="row form-section">
      	
      	<div id="contact-form" class="twelve columns">

            <form name="contactForm" id="contactForm" method="post" action="">

      			<fieldset>

                  <div class="row">
                    
                <div class="six columns mob-whole">   
<%= form_for(:ambassador, :url => {:action => "create"}, :html => {:multipart => true}) do |f| %>
            <%= f.text_field :name, class:"six columns mob-whole", placeholder: "Name" %> <br />
            <%= f.text_field :college, class:"six columns mob-whole", placeholder: "College" %> <br />
            <%= f.text_field :course, class:"six columns mob-whole", placeholder: "Course" %> <br />
            <%= f.email_field :email, class:"six columns mob-whole", placeholder: "Email" %><br />
            <%= f.text_field :year, class:"six columns mob-whole", placeholder: "Year" %> <br />
            <%= f.text_field :number, class:"six columns mob-whole", placeholder: "Number" %> <br />
	    <input type="submit" value="Send">
	 <% end %>
      <!-- /form-section -->   
                      </div>
                    </div>
                </fieldset>
            </form>
    </div>
    </div>
    
</section>


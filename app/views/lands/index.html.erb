   <!-- Contact Section
   ================================================== -->
   <section id="contact">

   	<div class="row section-head">

   		<div class="twelve columns">

	         <h1>Get In Touch With Us<span>.</span></h1>

	         <hr />

	         <p>For any queries, write to us!</p>

	      </div>

      </div>
    
        <div class="row form-section">
      	
      	<div id="contact-form" class="twelve columns">

            <form name="contactForm" id="contactForm" method="post" action="">

      			<fieldset>

                  <div class="row">
                    
                <div class="six columns mob-whole">      
       
  <%= form_for(:land, :url => {:action => "create"}, :html => {:multipart => true}) do |f| %>
            <%= f.text_field :firstname, class:"six columns mob-whole", placeholder: "First Name" %> <br />
            <%= f.text_field :lastname, class:"six columns mob-whole", placeholder: "Last Name" %> <br />
            <%= f.text_field :email, class:"six columns mob-whole", placeholder: "Email" %> <br />
            <%= f.text_field :subject, class:"six columns mob-whole", placeholder: "Subject" %><br />
            <%= f.text_area :message, class:"six columns mob-whole", placeholder: "Message" %> <br />
	    <input type="submit" value="Send">
	 <% end %>
      <!-- /form-section -->     

                      </div>
                    </div>
                </fieldset>
            </form>
            </div>
       </div>

   </section>  <!-- /contact-->


